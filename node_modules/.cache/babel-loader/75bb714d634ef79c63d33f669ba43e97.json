imit=5')\n              .then(res => this.todos = res.data)\n              .catch(err => console.log(err));\n    }\n  }\n</script>\n\n<style>\n  * {\n    box-sizing: border-box;\n    margin: 0;\n    padding: 0;\n  }\n\n  body{\n    font-family: Arial, Helvetiva, sans-serif;\n    line-height: 1.4;\n  }\n\n  .btn {\n    display: inline-block;\n    border: none;\n    background: #555;\n    color: #fff;\n    padding: 7px 20px;\n    cursor: pointer;\n  }\n  .btn:hover {\n    background: #666;\n  }\n</style>\n"],"sourceRoot":"src/views"}]}